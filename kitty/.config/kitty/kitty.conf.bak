font_family     CaskaydiaCove Nerd Font
font_size                  10
background_opacity         0.8
dynamic_background_opacity yes

map alt+l next_window
map alt+h previous_window
map alt+k previous_tab
map alt+j next_tab
map alt+y new_tab
map alt+u new_window
map alt+m close_window
map alt+n close_tab
map alt+c copy_to_clipboard
map alt+v paste_from_clipboard
map alt+e clear_terminal to_cursor active
map alt+q quit


startup_session ./default_session.conf



# BEGIN_KITTY_THEME
# Tokyo Night
include current-theme.conf
# END_KITTY_THEME